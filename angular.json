{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "wheelsage": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "i18n": {
        "sourceLocale": {
          "code": "en-GB",
          "baseHref": "/"
        },
        "locales": {
          "ru": {
            "translation": "src/locale/messages.ru.xlf",
            "baseHref": "/"
          },
          "uk": {
            "translation": "src/locale/messages.uk.xlf",
            "baseHref": "/"
          },
          "be-BY": {
            "translation": "src/locale/messages.be-BY.xlf",
            "baseHref": "/"
          },
          "fr": {
            "translation": "src/locale/messages.fr.xlf",
            "baseHref": "/"
          },
          "pt-BR": {
            "translation": "src/locale/messages.pt-BR.xlf",
            "baseHref": "/"
          },
          "zh-CN": {
            "translation": "src/locale/messages.zh-CN.xlf",
            "baseHref": "/"
          },
          "es": {
            "translation": "src/locale/messages.es.xlf",
            "baseHref": "/"
          }
        }
      },
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "allowedCommonJsDependencies": [
              "jquery",
              "sprintf-js",
              "showdown",
              "url-parse",
              "chart.js",
              "keycloak-js"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/browserconfig.xml",
              "src/manifest.json",
              "src/manifest.webapp",
              "src/yandex-browser-manifest.json",
              {
                "glob": "**/*",
                "input": "src/favicons",
                "output": "./favicons/"
              },
              {
                "glob": "**/*",
                "input": "src/languages",
                "output": "/i18n"
              },
              {
                "glob": "**/*",
                "input": "node_modules/leaflet/dist/images",
                "output": "./assets/"
              }
            ],
            "styles": [
              "node_modules/leaflet/dist/leaflet.css",
              "src/app/styles.scss",
              "src/app/brandicon.scss",
              "src/app/jcrop/jquery.Jcrop.scss",
              "src/assets/flags/flags.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js"
            ],
            "localize": [
              "be-BY",
              "en-GB",
              "fr",
              "pt-BR",
              "ru",
              "uk",
              "zh-CN",
              "es"
            ]
          },
          "configurations": {
            "serve": {
              "optimization": false,
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": false,
              "vendorChunk": false,
              "buildOptimizer": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.test.ts"
                }
              ],
              "localize": [
                "en-GB"
              ]
            },
            "test": {
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": false,
              "vendorChunk": false,
              "buildOptimizer": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.test.ts"
                }
              ]
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "browserTarget": "wheelsage:build"
          },
          "configurations": {
            "serve": {
              "browserTarget": "wheelsage:build:serve"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "wheelsage:build"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "ngx-build-plus:karma"
        }
      }
    }
  },
  "cli": {
    "schematicCollections": [
      "@angular-eslint/schematics"
    ],
    "analytics": "c1011e93-556a-4ca6-b2c1-2fdae8095785"
  }
}
